# Uncomment the next line to define a global platform for your project
platform :ios, '8.0'

  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

project 'Namdong.xcodeproj'

def shared_pods
  # Pods for NamdongCS
  pod 'Firebase/Core'
  pod 'Firebase/Messaging'
  pod 'Toaster', '~> 2.0'
  pod 'RNNotificationView', '~> 2.0'
end

target 'NamdongCS' do
  # Pods for NamdongCS
    shared_pods
end

target 'NamdongOP' do
    shared_pods
end

target 'DongyoungCS' do
    shared_pods
end

target 'DongyoungOP' do
    shared_pods
end